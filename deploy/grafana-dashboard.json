{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(revenge_http_requests_total[5m])) by (method)",
          "legendFormat": "{{method}}"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "options": {
        "legend": { "calcs": ["mean", "p99"], "displayMode": "table", "placement": "bottom" }
      },
      "title": "HTTP Request Latency (p99)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(revenge_http_request_duration_seconds_bucket[5m])) by (le, path))",
          "legendFormat": "{{path}}"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": { "unit": "short" }
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 },
      "id": 3,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "title": "Active Sessions",
      "type": "stat",
      "targets": [
        { "expr": "revenge_sessions_active_total" }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": { "unit": "short" }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 8 },
      "id": 4,
      "title": "Requests In Flight",
      "type": "stat",
      "targets": [
        { "expr": "revenge_http_requests_in_flight" }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "id": 5,
      "title": "Cache Hit/Miss Ratio",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(revenge_cache_hits_total[5m])) by (cache, layer)",
          "legendFormat": "hits: {{cache}} ({{layer}})"
        },
        {
          "expr": "sum(rate(revenge_cache_misses_total[5m])) by (cache, layer)",
          "legendFormat": "misses: {{cache}} ({{layer}})"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": { "unit": "s" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "id": 6,
      "title": "Database Query Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(revenge_db_query_duration_seconds_bucket[5m])) by (le, operation))",
          "legendFormat": "p99: {{operation}}"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "id": 7,
      "title": "Job Queue Status",
      "type": "timeseries",
      "targets": [
        {
          "expr": "revenge_jobs_queue_size",
          "legendFormat": "{{state}}"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": { "unit": "s" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "id": 8,
      "title": "Job Execution Duration",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(revenge_jobs_duration_seconds_bucket[5m])) by (le, job_type))",
          "legendFormat": "p99: {{job_type}}"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "id": 9,
      "title": "Database Pool Connections",
      "type": "timeseries",
      "targets": [
        { "expr": "db_pool_acquired_conns", "legendFormat": "acquired" },
        { "expr": "db_pool_idle_conns", "legendFormat": "idle" },
        { "expr": "db_pool_total_conns", "legendFormat": "total" },
        { "expr": "db_pool_max_conns", "legendFormat": "max" }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
      "id": 10,
      "title": "Authentication Attempts",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(revenge_auth_attempts_total[5m])) by (method, status)",
          "legendFormat": "{{method}}: {{status}}"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 36 },
      "id": 11,
      "title": "Rate Limit Hits",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(revenge_ratelimit_hits_total[5m])) by (limiter, action)",
          "legendFormat": "{{limiter}}: {{action}}"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": { "unit": "s" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 36 },
      "id": 12,
      "title": "Search Query Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(revenge_search_query_duration_seconds_bucket[5m])) by (le, type))",
          "legendFormat": "p99: {{type}}"
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["revenge", "media-server"],
  "templating": {
    "list": [
